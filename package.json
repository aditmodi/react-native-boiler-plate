{
	"name": "reacNativeDemoApp",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"start": "node node_modules/react-native/local-cli/cli.js start",
		"test": "rm -rf ./node_modules/jest-cli/.haste_cache && jest",
		"android-linux": "react-native bundle --platform android --dev false --entry-file index.android.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && react-native run-android"
	},
	"dependencies": {
		"@remobile/react-native-splashscreen": "^1.0.7",
		"cloudinary": "^1.9.0",
		"native-base": "^2.3.2",
		"prop-types": "^15.6.0",
		"react": "16.0.0-alpha.12",
		"react-native": "^0.49.1",
		"react-native-android-back-button": "^0.3.0",
		"react-native-android-location-services-dialog-box": "^2.2.1",
		"react-native-camera": "^0.10.0",
		"react-native-fetch-blob": "^0.10.8",
		"react-native-fs": "^2.5.2",
		"react-native-geocoding": "^0.2.0",
		"react-native-indicators": "^0.10.0",
		"react-native-loading-spinner-overlay": "^0.5.2",
		"react-native-maps": "^0.16.4",
		"react-native-side-menu": "^1.1.3",
		"react-native-simple-radio-button": "^2.6.2",
		"react-native-splash-screen": "^3.0.1",
		"react-navigation": "^1.0.0-beta.13"
	},
	"devDependencies": {
		"babel-jest": "21.2.0",
		"babel-preset-react-native": "4.0.0",
		"jest": "21.2.1",
		"jest-cli": "^21.2.1",
		"react-addons-test-utils": "^15.6.2",
		"react-test-renderer": "16.0.0-alpha.12"
	},
	"jest": {
		"preset": "react-native",
		"scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
		"testFileExtensions": [
			"es6",
			"js"
		],
		"moduleFileExtensions": [
			"js",
			"json",
			"es6"
		],
		"unmockedModulePathPatterns": [
			"<rootDir>/node_modules/react",
			"<rootDir>/node_modules/fbjs"
		],
		"verbose": true,
		"collectCoverage": true
	}
}
